<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
 <link rel="stylesheet" href="./styles.css">

<div class="row">
{{#each profs}}
  <div class="col-4">
    <div class="card mt-5 mb-3" id="card-info" style="width: 300px; height: 500px; margin-top: 1%">
  <div class="card-body">
    <h5 class="card-title">My Beta </h5>
<img src={{ profile_image }} style="width: 250px; height: auto; margin-top:1%">
  </div>
  <ul class="list-group list-group-flush">
    <li class="list-group-item">Username: {{user.username }}</li>
    <li class="list-group-item">Email: {{user.email}}</li>
    <li class="list-group-item">Location: {{user_location}}</li>
    <li class="list-group-item">Experience: {{user_experience }}</li>
    <li class="list-group-item">Gear: {{ has_gear }}</li>
      </ul>
 </div>
</div>
 {{/each}}

